{
    "extends": "@sourcegraph/eslint-config",
    "rules": {
      "import/no-default-export": "off",
      "react/forbid-dom-props": "warn",
      "@typescript-eslint/require-await": "warn",
      "multiline-ternary": "error",
      "no-console": ["error",
        { "allow": ["error"] }
      ]
    },
    "parserOptions": {
      "project": "tsconfig.json"
    }
}
